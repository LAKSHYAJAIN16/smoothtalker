{
    "name": "Smoothtalker",
    "version": "1.0.0",
    "description": "Talking has never been easier",
    "manifest_version": 3,
    "author": "Lakshya Jain",
    "action": {
        "default_popup": "index.html",
        "default_title": "Smoothtalker"
    },
    "permissions": [
        "tabs",
        "<all_urls>",
        "scripting",
        "http://*/",
        "https://*/"
    ],
    "host_permissions": ["https://*/*"]
}